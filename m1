#include<stdio.h>
int main(){
int  m[5][5];
int i,j,n;
for(i=0;i<n;i++)
for(j=0;j<n;j++)
printf("donner la matrix:\n");
scanf("%d",&m[i][j]);
for(i=0;i<n;i++){
for(j=n;j<0;j--){
printf("diagonale lamatrix %d",m[i][j]);
}
}
for(i=0;i<n;i++)
for(j=0;j<n;j++)
if(i=j)
printf("diagonale la matrix %d",m[i][j]);
return 0;
}

